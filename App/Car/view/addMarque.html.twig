{% extends "admin.layout.html.twig" %}

{% block stylesheet %}
    <link rel = "stylesheet" href="{{ assets('/assets/css/admin.css')}}" />
{% endblock %}

{% block body %}
<!-- La méthode POST récupére le 'name' de l'input
    enctype="multipart/form-data" permet de récuperer l'image envoyer
     // Obligatoire pour récupérer un fichier -->
<form method="POST" action="" class="card p-4" style="max-width: 500px;">

    <div class="input-group input-group-sm mb-3">
        <span class="input-group-text" id="inputGroup-sizing-sm">Marque</span>
        <input type="text" name="marque" class="form-control" aria-label="Sizing example input"
            aria-describedby="inputGroup-sizing-sm">
    </div>

    <input type="submit" value="Ajouter">

</form>
{% endblock %}